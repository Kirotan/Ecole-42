{
	"files.associations": {
		"limits.h": "c",
		"mlx42.h": "c",
		"libft.h": "c",
		"math.h": "c",
		"fdf.h": "c",
		"array": "c",
		"functional": "c",
		"unistd.h": "c",
		"fcntl.h": "c",
		"stdio.h": "c",
		"stdlib.h": "c"
	},
	"cmake.configureOnOpen": true,
	"editor.fontSize": 28,
	"editor.tokenColorCustomizations": {

		"[*Light*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#000"
					}
				}
			]
		},
		"[*Dark*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#fff"
					}
				}
			]
		}
	},
	"explorer.openEditors.visible": 9
}
